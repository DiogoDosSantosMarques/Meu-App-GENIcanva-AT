<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appgenicanva">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppGenicanva">


        <activity
            android:name=".fragmentos.Geni_mais_Canva"
            android:exported="true"
            android:parentActivityName=".fragmentos.ComoFuncionar" />

        <activity
            android:name=".fragmentos.Tela_Inicio"
            android:exported="true"
            android:parentActivityName=".fragmentos.ComoFuncionar" />
        <activity
            android:name=".fragmentos.Apresentacao"
            android:exported="false" />
        <activity
            android:name=".fragmentos.Estrategias"
            android:exported="false" />
        <activity
            android:name=".fragmentos.GameQuiz"
            android:exported="false" />
        <activity
            android:name=".fragmentos.Pratique_Tela"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>